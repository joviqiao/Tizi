	<body class="chinese">
		<div id="wrapper">
			<div class="masthead comtainheit">
				<div class="logoline">
					<div class="logocont">
						<img src="static/tizi.480.png" />
						<p>爬出墙，让你的世界自由呼吸！</p>
						<div class="clearfix"></div>
					</div>
				</div>
				<div class="container1 ">
					<div class="home-signup">
						<div class="login-box">
							<a href="javascript:;" class="signup tab col-xs-6 ng-binding active">注册</a>
							<a href="javascript:;" class="signin tab col-xs-6 ng-binding ">登录</a>
							<script type="text/javascript">
								$(function(){
									<% if(!!page && page === "signup"){ %>
										$(".signup").addClass("active");
										$(".signup").click();
									<% }else if(!!page && page === "signin"){ %>
										$(".signin").addClass("active");
										$(".signin").click();
									<% }%>
									<% if(!!page && page === "signup" && !!error){ %>
										$(".upemail").addClass("has-error");
										$(".upemail input").addClass("has-error");
									<% }else if(!!page && page === "signin" && !!error){ %>
										$(".inemail").addClass("has-error");
										$(".upemail input").addClass("has-error");
									<% }%>
									<% if(!!error && error === "exist"  && !!email){ %>
										$(".upemail input").val("<%=email%>");
										setTimeout(function(){
											$(".signup").removeClass("active");
											$(".signin").addClass("active");
											$(".signin").click();
											$(".inemail input").val("<%=email%>");
											$("#inuser_password").focus();
										},3000);										
									<% }%>
									$(".signupform").submit(function(e){
									    if($("#upuser_password").val().length < 4){
									    	$(".uppassword").addClass("has-error");
									    	return false;
									    }
									    if($("#uprepassword").val() !== $("#upuser_password").val()){
									    	$(".uprepassword").addClass("has-error");
									    	return false;
									    }
									});
									$("#upuser_password").blur(function(){
										if($("#upuser_password").val().length >= 4){
											$(".uppassword").removeClass("has-error");
										}
									});
									$("#uprepassword").blur(function(){
										if($("#uprepassword").val() === $("#upuser_password").val()){
											$(".uprepassword").removeClass("has-error");
										}
									});
								})
							</script>
							<form class="step signupform" action="#" method="post">
								<div class="form-group upemail ">
									<div class="errors">
										<span class="notExisted"><%=info%><i class="fa fa-info-circle"></i></span>
									</div>
									<label class="input-opt">邮箱</label>
									<input class="font-kai input-lg form-control has-error" id="upuser_name" name="email" placeholder=" 请输入邮箱" type="text" />
								</div>
								<div class="form-group uppassword">
									<div class="errors">
										<span class="notExisted">密码太少了，至少四个字符！<i class="fa fa-info-circle"></i></span>
									</div>
									<label class="input-opt">密码</label>
									<input class="font-kai input-lg form-control has-error" id="upuser_password" name="password" placeholder=" 请输入密码" type="password" />
								</div>
								<div class="form-group uprepassword">
									<div class="errors">
										<span class="notExisted">和上面输入不一致！<i class="fa fa-info-circle"></i></span>
									</div>
									<label class="input-opt">确认密码</label>
									<input class="font-kai input-lg form-control has-error" id="uprepassword" name="repassword" placeholder=" 请重复上面的密码" type="password" />
								</div>
								<button type="submit" class="btn btn-block btn-success btn-lg">
									<span>注册</span>
								</button>
								<div class="clearfix"></div>
							</form>

							<form class="step signinform" action="#" method="post" style="display: none;">
								<div class="form-group inemail">
									<div class="errors">
										<span class="notExisted"><%=info%><i class="fa fa-info-circle"></i></span>
									</div>
									<label class="input-opt">邮箱</label>
									<input class="font-kai input-lg form-control " id="inuser_name" name="email" placeholder=" 请输入邮箱" type="text" />
								</div>
								<div class="form-group inpassword ">
									<div class="errors">
										<span class="notExisted"><i class="fa fa-info-circle"></i></span>
									</div>
									<label class="input-opt">密码</label>
									<input class="font-kai input-lg form-control has-error" id="inuser_password" name="password" placeholder=" 请输入密码" type="password" />
								</div>
								<button type="submit" class="btn btn-block btn-primary btn-lg">
									<span>登录</span>
								</button>
								<a href="" class="pull-right">找回密码</a>
								<div class="clearfix"></div>
							</form>
						</div>
